{
  "name": "acr",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production cross-env NODE_OPTIONS=--no-deprecation next build --experimental-build-mode compile",
    "postbuild": "next-sitemap",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "NODE_ENV=production cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@payloadcms/db-postgres": "^3.39.1",
    "@payloadcms/live-preview-react": "^3.31.0",
    "@payloadcms/next": "^3.39.1",
    "@payloadcms/payload-cloud": "^3.39.1",
    "@payloadcms/richtext-lexical": "^3.39.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "gsap": "^3.13.0",
    "next": "15.2.3",
    "next-sitemap": "^4.2.3",
    "payload": "^3.39.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "yup": "^1.0.0-alpha.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.2",
    "prettier": "^3.4.2",
    "sass": "^1.86.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}